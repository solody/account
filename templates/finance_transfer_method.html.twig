{#
/**
 * @file finance_transfer_method.html.twig
 * Default theme implementation to present Transfer method data.
 *
 * This template is used when viewing Transfer method pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_finance_transfer_method()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('finance_transfer_method') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
